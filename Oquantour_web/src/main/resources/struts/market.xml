<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
        "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
    <package name="market" extends="json-default,struts-default">
        <action name="getMarketInfo" class="oquantour.action.MarketAction" method="getMarketInfo">
            <result type="json">
                <param name="root">result</param>
            </result>
        </action>

        <action name="getTopListInfo" class="oquantour.action.MarketAction" method="getTopListInfo">
            <result type="json">
                <param name="root">result</param>
            </result>
        </action>

    </package>
</struts>